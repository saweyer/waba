// Presently (as of Beta 3) we're not using this code -- Sean/* Check the code below to see if your favorite script is represented, and   with the correct number of arguments.  If not, send mail to sean@cs.gmu.edu,   and we'll add it in for you.  These scripts represent all expressly-stated   public functions that specify a possible need to call the inherited version.   In all cases, it'll call the script with the appropriate number of arguments;   if you have the wrong size args array, you'll get an exception thrown and   the only way you'll find out about it is that you'll get a print-out to the   inspector.   nil is returned on an exception or if the inherited script   can't be found that you requested.  */// This is gonna be SLOW, due to all the "else ifs".  Oh well.DefineGlobalConstant('InheritedScripts,func(input /* a symbol representing the function name */ ,	 args /* an array of arguments */)begintry // 2.0 Newton Programmer's Reference items	if (input='ViewSetupFormScript) then inherited:?ViewSetupFormScript();	else if (input='ViewSetupChildrenScript) then inherited:?ViewSetupChildrenScript();	else if (input='ViewSetupDoneScript) then inherited:?ViewSetupDoneScript();	else if (input='ViewQuitScript) then inherited:?ViewQuitScript();	else if (input='ViewPostScript) then inherited:?ViewPostScript();	else if (input='ViewShowScript) then inherited:?ViewShowScript();	else if (input='ViewHideScript) then inherited:?ViewHideScript();	else if (input='ViewDrawScript) then inherited:?ViewDrawScript();	else if (input='ViewHiliteScript) then inherited:?ViewHiliteScript(args[0]);	else if (input='ViewScrollDownScript) then inherited:?ViewScrollDownScript();	else if (input='ViewScrollUpScript) then inherited:?ViewScrollUpScript();	else if (input='ViewOverviewScript) then inherited:?ViewOverviewScript();	else if (input='ViewAddChildScript) then inherited:?ViewAddChildScript(args[0]);	else if (input='ViewChangedScript) then inherited:?ViewChangedScript(args[0],args[1]);	else if (input='ViewDropChildScript) then inherited:?ViewDropChildScript(args[0]);	else if (input='ViewIdleScript) then inherited:?ViewIdleScript();	else if (input='viewDrawDragDataScript) then inherited:?viewDrawDragDataScript(args[0]);	else if (input='ViewDrawDragBackgroundScript) then inherited:?ViewDrawDragBackgroundScript(args[0],args[1]);	else if (input='ViewGetDropTypesScript) then inherited:?ViewGetDropTypesScript(args[0]);	else if (input='ViewFindTargetScript) then inherited:?ViewFindTargetScript(args[0]);	else if (input='ViewDropApproveScript) then inherited:?ViewDropApproveScript(args[0]);	else if (input='ViewDragFeedbackScript) then inherited:?ViewDragFeedbackScript(args[0],args[1],args[2]);	else if (input='ViewGetDropDataScript) then inherited:?ViewGetDropDataScript(args[0],args[1]);	else if (input='ViewDropScript) then inherited:?ViewDropScript(args[0],args[1],args[2]);	else if (input='ViewDropMoveScript) then inherited:?ViewDropMoveScript(args[0],args[1],args[2],args[3]);	else if (input='ViewDropRemoveScript) then inherited:?ViewDropRemoveScript(args[0]);	else if (input='ViewDropDoneScript) then inherited:?ViewDropDoneScript();	else if (input='ViewClickScript) then inherited:?ViewClickScript(args[0]);	else if (input='HitItem) then inherited:?HitItem(args[0],args[1],args[2]);	else if (input='ButtonClickScript and (args=nil or Length(args)=0)) then inherited:?ButtonClickScript();	else if (input='ButtonClickScript) then inherited:?ButtonClickScript(args[0]);	else if (input='PickActionScript) then inherited:?PickActionScript(args[0]);	else if (input='PickCancelledScript) then inherited:?PickCancelledScript();	else if (input='ButtonToggleScript) then inherited:?ButtonToggleScript(args[0]);	else if (input='SelectThisCell) then inherited:?SelectThisCell(args[0]);	else if (input='TabSetup) then inherited:?TabSetup(args[0],args[1],args[2]);	else if (input='DoScrollScript) then inherited:?DoScrollScript(args[0]);	else if (input='ReorientToScreen) then inherited:?ReorientToScreen();	else if (input='ModifyEntryPath) then inherited:?ModifyEntryPath(args[0],args[1],args[2]);	else if (input='SaveUndoState) then inherited:?SaveUndoState(args[0]);	else if (input='Setup) then inherited:?Setup(args[0],args[1],args[2]);	else if (input='GetTargetInfo) then inherited:?GetTargetInfo(args[0]);	else if (input='Delete) then inherited:?Delete();	else if (input='FileAndMove) then inherited:?FileAndMove(args[0],args[1],args[2],args[3]);	else if (input='LabelActionScript) then inherited:?LabelActionScript();	else if (input='SetupItem) then inherited:?SetupItem(args[0],args[1]);	else if (input='IgnoreError) then inherited:?IgnoreError(args[0]);	else if (input='InstallScript) then inherited:?InstallScript(args[0]);	else if (input='NewItem) then inherited:?NewItem(args[0]);	else if (input='TranslateError) then inherited:?TranslateError(args[0]);// 2.1 Addendum items:	else if (input='ToolClickScript) then inherited:?ToolClickScript(args[0]);	else if (input='MatchedPassword) then inherited:?MatchedPassword();	// failed to match anything...Print out something pretty and return nil.	else begin 		Print("Waba NewtonScript API: Do not know how to call inherited function '" && 			   SPrintObject(input) && "'"); 		nil 		end;		// exception generated, either inside the inherited function,// or because we tried to call with the wrong number of args or// with nil args.  Print out something pretty and return nil.onexception |evt.ex| do begin		Print("Waba NewtonScript API: Calling inherited function '" && 		       SPrintObject(input) && "' with args (" && SPrintObject(args) && 			   ") created the following error:\n" && CurrentException());		nil 		end;end);